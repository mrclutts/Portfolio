<!DOCTYPE html>


    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Matt Clutts</title>
    @*@Styles.Render("~/Content/css")*@
       
    <!--BootStrap CSS-->
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <!--Fonts-->
    <link href="~/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Arvo:400,700' rel='stylesheet' type='text/css'>
    <!--Custom CSS-->
    <link href="~/Content/css/general.css" rel="stylesheet" />
    <link href="~/Content/css/blogstyle.css" rel="stylesheet" />
    <!--Carousel CSS-->
    <link href="~/Content/css/custom.css" rel="stylesheet" />
    <link href="~/Content/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/css/owl.theme.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/animate.css" rel="stylesheet" />
    <link rel="icon" href="@Url.Content("~/tree_favicon.gif")" />
 

    <!--PopUp CSS--> 
    <link href="~/Content/css/magnific-popup.css" rel="stylesheet" />

    <!-- SyntaxHighlighter CSS-->
    <link href="~/Content/SyntaxHighlighter/shCoreEclipse.css" rel="stylesheet" />
    <link href="~/Content/SyntaxHighlighter/shThemeEclipse.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")

    <!-- NavBar-->
    <nav class="navbar-default" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/Home/Index">Home</a>
            </div>

            <div class="collapse navbar-collapse navbar-right navbar-ex1-collapse">
                <ul class="nav navbar-nav">

                    <li class="menuItem @(ViewContext.RouteData.Values["Action"].ToString() == "About" ? "active" : "")">@Html.ActionLink("About", "About", "Home")</li>
                    <li class="menuItem @(ViewContext.RouteData.Values["Action"].ToString() == "Resume" ? "active" : "")"><a target="_blank" href="~/img/Matthew Clutts Resume.pdf">Resume</a></li>
                    <li class="menuItem @(ViewContext.RouteData.Values["Action"].ToString() == "Portfolio" ? "active" : "")">@Html.ActionLink("Portfolio", "Portfolio", "Home")</li>                   
                    <li class="menuItem @(ViewContext.RouteData.Values["Action"].ToString() == "Blog" ? "active" : "")">@Html.ActionLink("Blog", "Index", "Blogs") </li>
                    <li class="menuItem @(ViewContext.RouteData.Values["Action"].ToString() == "Music" ? "active" : "")">@Html.ActionLink("Music", "Music", "Home") </li>
                    <li class="menuItem @(ViewContext.RouteData.Values["Action"].ToString() == "Contact" ? "active" : "")">@Html.ActionLink("Contact", "Contact", "Home")</li>

                </ul>
            </div>

        </div>
    </nav>
    <div class="body-content">
        @RenderBody()
        <!-- Preloader -->
        <div id="preloader">
            <div id="status"></div>
        </div>
        <footer>
            
            <div class="row">
                <div class="container">
                    
                        <div class="col-md-5 col-xs-12 nopad">
                             <div class="footer-banner">
                            <h3 class="footer-title">Built with:</h3>
                            <ul>
                                <li>
                                    .Net Framework
                                    <img src="~/img/whiteIcon/prog-csharp.png" />
                                </li>
                                <li>
                                    HTML5
                                    <img src="~/img/whiteIcon/html5-02.png" />
                                </li>
                                <li>
                                    CSS3
                                    <img src="~/img/whiteIcon/css3-02.png" />
                                </li>
                                <li>
                                    JavaScript
                                    <img src="~/img/whiteIcon/prog-js01.png" />
                                </li>
                                <li>
                                    JQuery
                                    <img src="~/img/whiteIcon/prog-jquery.png" />
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-7 col-xs-12">
                        <h3 class="footer-title">More of my work:</h3>
                        <p>
                            <br />
                            <a href="http://secroall.azurewebsites.net/" target="_blank">Car Finder Web Application</a>
                            <br />
                            <a href="http://secroall.azurewebsites.net/" target="_blank">Stephanie Croall Art Portfolio</a>
                        </p>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
   
    <!-- JavaScript -->
    <script src="~/js/jquery-1.10.2.js"></script>
    <script src="~/js/bootstrap.js"></script>  
    
<script src="~/js/looper.js"></script>

    <script src="~/js/owl.carousel.js"></script>   
    <script src="~/js/portfolio.js"></script>
    
    <!-- StikyMenu -->
    @*<script src="~/js/stickUp.min.js"></script>*@
    <script src="~/js/script.js"></script>

    <!-- Smoothscroll -->
    <script type="text/javascript" src="~/js/jquery.corner.js"></script>
    <script src="~/js/wow.min.js"></script>
    <script>
        $(function () {
            new WOW().init();
        });	 
    </script>
    <script src="~/js/classie.js"></script>
  
    <!-- Magnific Popup core JS file -->
    <script src="~/js/jquery.magnific-popup.js"></script>
    <script src="~/Scripts/SyntaxHighlighter/shCore.js"></script>
    <script src="~/Scripts/SyntaxHighlighter/shBrushJScript.js"></script>
    <script type="text/javascript">
        SyntaxHighlighter.all();
    </script>

    

 @RenderSection("scripts", required: false)

