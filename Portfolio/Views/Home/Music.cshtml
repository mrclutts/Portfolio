@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!--Music-->
<section id="music" class="content-section text-center">
    <div class="container">
        <div class="col-md-12 col-xs-12 wow fadeInDownBig">
            <h2>What I'm Listening To</h2>
